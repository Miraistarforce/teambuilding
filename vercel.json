{
  "version": 2,
  "builds": [
    {
      "src": "timecard-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "timecard-app/dist"
      }
    },
    {
      "src": "store-admin-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "store-admin-app/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/store-admin-app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/timecard-app/$1"
    }
  ]
}